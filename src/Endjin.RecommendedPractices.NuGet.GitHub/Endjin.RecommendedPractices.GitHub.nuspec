        <description>Applies endjin standard practices to a .NET project hosted in GitHub.</description>
        <authors>endjin</authors>

        <developmentDependency>true</developmentDependency>
        <dependencies>
            <dependency id="Microsoft.SourceLink.GitHub" version="1.1.1" />
            <dependency id="Endjin.RecommendedPractices" version="$version$" />
        </dependencies>
    </metadata>

    <files>
        <file src="build\**" target="build" />
        <file src="build\**" target="buildMultiTargeting" />
    </files>
</package>