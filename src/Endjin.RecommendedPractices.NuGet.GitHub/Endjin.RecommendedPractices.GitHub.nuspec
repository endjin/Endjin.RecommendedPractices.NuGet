<?xml version="1.0"?>
<package xmlns="http://schemas.microsoft.com/packaging/2013/05/nuspec.xsd">

    <metadata>
        <id>Endjin.RecommendedPractices.GitHub</id>

        <!-- This is overridden by the automatic build. It is present here to avoid validation failures. -->
